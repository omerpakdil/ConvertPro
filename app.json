{
  "expo": {
    "name": "ConvertPro",
    "slug": "convertpro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.callousity.convertpro"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "navigationBarColor": "#1a1a2e",
      "statusBarBackgroundColor": "#1a1a2e",
      "package": "com.callousity.convertpro"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-secure-store",
      "expo-document-picker",
      [
        "expo-media-library",
        {
          "photosPermission": "ConvertPro needs access to save your compressed images to Photos app so you can easily find and share them.",
          "savePhotosPermission": "ConvertPro will save your compressed images to Photos app for easy access.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ba22128b-05f8-462b-bf7b-f4dd908b209f"
      }
    }
  }
}
